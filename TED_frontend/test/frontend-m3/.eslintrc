{
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": [
      1,
      "tab"
    ],
    "linebreak-style": [
      1,
      "unix"
    ],
    "quotes": [
      1,
      "double"
    ],
    "semi": [
      1,
      "always"
    ],
    "react/prop-types": 1,
    "no-console": 1,
    "no-debugger": 1
  }
}
